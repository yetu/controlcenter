{
  "name": "controlcenter",
  "version": "0.0.0",
  "dependencies": {
    "es5-shim": "^4.1.0",
    "react": "^0.12.2",
    "react-router": "^0.12.4",
    "reflux": "^0.2.5"
  },
  "devDependencies": {
    "autoprefixer-loader": "1.1.0",
    "css-loader": "^0.9.1",
    "csso": "^1.3.11",
    "eslint": "^0.15.1",
    "extract-text-webpack-plugin": "^0.3.8",
    "file-loader": "^0.8.1",
    "husky": "^0.6.2",
    "istanbul": "^0.3.5",
    "istanbul-instrumenter-loader": "^0.1.2",
    "jade-loader": "^0.7.0",
    "jasmine-core": "^2.1.3",
    "jasmine-reporters": "~0.2.1",
    "jsx-loader": "^0.12.2",
    "karma": "^0.12.28",
    "karma-chrome-launcher": "^0.1.7",
    "karma-coverage": "^0.2.7",
    "karma-firefox-launcher": "^0.1.4",
    "karma-jasmine": "^0.3.3",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-webpack": "^1.3.1",
    "loader-utils": "^0.2.6",
    "node-sass": "^2.0.1",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^1.1.7",
    "sass-graph": "^1.0.3",
    "sass-loader": "^0.4.0",
    "source-map": "^0.4.0",
    "style-loader": "^0.8.3",
    "url-loader": "^0.5.5",
    "webpack": "^1.4.15",
    "webpack-dev-server": "^1.7.0"
  },
  "engines": {
    "node": ">=0.8.0"
  },
  "scripts": {
    "clean": "rm -rf ./public/app/dist && rm -rf ./public/dist ",
    "bundle": "webpack -p --config ./public/webpack.config.js ",
    "bundle-dev": "webpack -d -w --config ./public/webpack.config.js",
    "start" : "node public/hot-reload-server.js",
    "js-files": "find ./public \\( -name '*.js' -o -name '*.jsx' \\) ! -path './node_modules/*' ! -path './public/bower_components/*' ! -path './coverage/*' ! -path './public/dist/*' ! -path './jspm_packages'",
    "test": "npm run bundle && node ./node_modules/karma/bin/karma start ./public/app/project-setup/test/karma.conf.js --single-run",
    "tdd": "npm run bundle-dev && node ./node_modules/karma/bin/karma start ./public/app/project-setup/test/karma.conf.js",
    "build": "npm run clean && npm run bundle && cp -r ./public/app/* ./public/dist",
    "eslint": "node_modules/.bin/eslint `npm run -s js-files`",
    "lint": "npm run eslint",
    "precommit": "npm test"
  }
}
